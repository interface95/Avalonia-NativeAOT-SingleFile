<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key='Light'>
            <SolidColorBrush x:Key='WindowContainerContentBackgroundBrush'>#88FFFFFF</SolidColorBrush>
            <SolidColorBrush x:Key='WindowContainerTitleBarBackgroundBrush'>Transparent</SolidColorBrush>
        </ResourceDictionary>
        <ResourceDictionary x:Key='Dark'>
            <SolidColorBrush x:Key='WindowContainerContentBackgroundBrush'>#88000000</SolidColorBrush>
            <SolidColorBrush x:Key='WindowContainerTitleBarBackgroundBrush'>#44000000</SolidColorBrush>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>